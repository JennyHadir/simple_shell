#include <stdio.h>
#include <stdlib.h>
#include <sys/types.h>
#include <fcntl.h>
#include <unistd.h>
#include <string.h>
/**
 * main - read line
 *
 * Return: Always 0.
 */
int main(void)
{
int bt_read;
char *str;
size_t * __restrict__ size;
 size_t s;
printf("$");
 str = (char *) malloc (s);
bt_read = getline(&str, size, stdin);
if (bt_read == -1)
  return (-1);
return (0);
}
